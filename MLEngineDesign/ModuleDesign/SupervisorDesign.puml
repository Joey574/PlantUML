@startuml Supervisor
package "Supervisor" {
    struct Supervisor {

        - model : NeuralNetwork*
        - dataset : Dataset*

        - config : YAML::Node*

        - path : string
        - name : string

        + Supervisor()
        + ~Supervisor()

        + Define(YAML::Node& config) : void

        + Load(const string& path, const string& name) : void
        - Save(const string& path, const string& name) : void

        + Train() : nlohmann::json
    }
}
@enduml
